{
  "name": "dentrino-web",
  "version": "0.0.0",
  "node": ">=16.16.0",
  "private": true,
  "scripts": {
    "db-up": "firebase emulators:start",
    "dev-build": "webpack --config ./webpack.config.js --mode development",
    "dev-up": "npm run dev-build && node -r dotenv/config -r @babel/register src/boot.js",
    "dev-start": "nodemon --inspect --exec 'npm run dev-up'",
    "sign": "npm run dev-build && node -r dotenv/config -r @babel/register -r ./src/config/config.js ./scripts/sign.js",
    "build": "webpack --config ./webpack.config.js --mode production",
    "build-backend": "webpack --config ./webpack.config.server.js --mode production",
    "start": "npm run build && rm -rf ./assets && cp -rf ./dist/* ./ && node src/start.js",
    "console": "node -r dotenv/config -r @babel/register -r ./src/config/config.js",
    "script": "npm run dev-build && node -r dotenv/config -r @babel/register -r ./src/config/config.js ./scripts/$SCRIPT.js",
    "test:run": "jest -i",
    "test": "firebase emulators:exec 'npm run test:run'",
    "func:deploy": "firebase -P dentrino-staging deploy --only functions:simulations_api"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.1.2",
    "@google-cloud/storage": "^6.5.4",
    "@sentry/node": "^7.16.0",
    "@sentry/tracing": "^7.16.0",
    "compression": "^1.7.4",
    "connect-timeout": "^1.9.0",
    "cookie-parser": "^1.4.5",
    "core-js": "^3.25.5",
    "debug": "^4.3.4",
    "ev-emitter": "^1.1.0",
    "express": "^4.18.2",
    "file-type": "^16.5.3",
    "firebase": "^9.10.0",
    "firebase-admin": "^11.0.1",
    "formidable": "^2.0.1",
    "global": "^4.4.0",
    "hbs": "^4.0.6",
    "helmet": "^6.0.0",
    "http-errors": "^2.0.0",
    "js-sha1": "^0.6.0",
    "js-sha256": "^0.9.0",
    "morgan": "^1.9.1",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "on-finished": "^2.4.1",
    "on-headers": "^1.0.2",
    "redis": "^2.8.0",
    "regenerator-runtime": "^0.13.7",
    "roddeh-i18n": "^1.2.0",
    "staticify": "^5.0.0",
    "uuid": "^9.0.0",
    "whatwg-fetch": "^3.6.2",
    "winston": "^3.2.1",
    "ws": "^8.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-private-methods": "^7.12.1",
    "@babel/plugin-proposal-private-property-in-object": "^7.12.1",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@firebase/rules-unit-testing": "^2.0.4",
    "asap": "^2.0.6",
    "axios": "^1.1.3",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.5",
    "dotenv": "^16.0.3",
    "dotenv-webpack": "^8.0.1",
    "firebase-tools": "^11.15.0",
    "form-data": "^4.0.0",
    "google-gax": "^3.5.1",
    "jest": "^29.2.0",
    "nodemon": "^2.0.20",
    "remove-files-webpack-plugin": "^1.1.3",
    "rosie": "^2.0.1",
    "supertest": "^6.3.0",
    "webpack": "^4.39.3",
		"exports-loader": "^1.1.1",
    "webpack-cli": "^4.10.0",
    "webpack-node-externals": "^3.0.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "corejs": "3.25.5",
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-private-methods",
      "@babel/plugin-proposal-private-property-in-object"
    ]
  },
  "nodemonConfig": {
    "ext": "js css hbs html json",
    "ignore": [
      "test/*",
      "public/*",
      "stress-test/*",
      "docs/*",
      "tmp/*",
      "*.log"
    ],
    "delay": "2500"
  }
}
