{
  "name": "dentrino-web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "db-up": "firebase emulators:start --only database",
    "dev-build": "webpack --config ./webpack.config.js --mode development",
    "dev-up": "npm run dev-build && node -r dotenv/config -r @babel/register -r regenerator-runtime/runtime src/boot.js",
    "dev-start": "nodemon --exec 'npm run dev-up'",
    "build": "webpack --config ./webpack.config.js --mode production",
    "build-backend": "npm run build && webpack --config ./webpack.config.server.js --mode production",
    "start": "node -r regenerator-runtime/runtime src/start.js",
    "console": "node -r dotenv/config -r @babel/register -r regenerator-runtime/runtime -r ./src/config.js",
    "new-access-point": "npm run dev-build && node -r dotenv/config -r @babel/register -r regenerator-runtime/runtime -r ./src/config.js ./scripts/new_access_point.js",
    "del-access-point": "npm run dev-build && node -r dotenv/config -r @babel/register -r regenerator-runtime/runtime -r ./src/config.js ./scripts/del_access_point.js",
    "prod-console": "DOTENV_CONFIG_PATH=.env.prod node -r dotenv/config -r @babel/register -r regenerator-runtime/runtime -r ./src/config.js",
    "prod-new-access-point": "DOTENV_CONFIG_PATH=./.env.prod npm run dev-build && node -r dotenv/config -r @babel/register -r regenerator-runtime/runtime -r ./src/config.js ./scripts/new_access_point.js",
    "prod-del-access-point": "DOTENV_CONFIG_PATH=./.env.prod npm run dev-build && node -r dotenv/config -r @babel/register -r regenerator-runtime/runtime -r ./src/config.js ./scripts/del_access_point.js",
    "test": "jest -i"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "compression": "~1.7.4",
    "helmet": "~3.20.0",
    "@godaddy/terminus": "~4.1.2",
    "aws-sdk": "~2.509.0",
    "@google-cloud/storage": "~5.3.0",
    "firebase-admin": "~8.3.0",
    "regenerator-runtime": "~0.13.3",
    "uuid": "~3.3.3",
    "js-sha1": "~0.6.0",
    "js-sha256": "~0.9.0",
    "cron": "~1.7.2",
    "ws": "~7.2.0",
    "redis": "~2.8.0",
    "winston": "~3.2.1",
    "roddeh-i18n": "~1.2.0",
    "@sendgrid/mail": "~6.5.2",
    "@sentry/node": "~5.15.2"
  },
  "devDependencies": {
    "nodemon": "~1.19.1",
    "@babel/core": "~7.5.5",
    "@babel/node": "~7.5.5",
    "@babel/cli": "~7.5.5",
    "@babel/preset-env": "~7.5.5",
    "@babel/register": "~7.5.5",
    "@babel/plugin-proposal-class-properties": "~7.12.1",
    "@babel/plugin-proposal-private-methods": "~7.12.1",
    "@babel/plugin-proposal-private-property-in-object": "~7.12.1",
    "babel-loader": "~8.0.6",
    "webpack": "~4.39.1",
    "webpack-cli": "~3.3.6",
    "copy-webpack-plugin": "~5.0.4",
    "remove-files-webpack-plugin": "~1.1.3",
    "dotenv": "~8.0.0",
    "webpack-node-externals": "~1.7.2",
    "jest": "~24.9.0",
    "rosie": "~2.0.1",
    "supertest": "~4.0.2",
    "@firebase/testing": "~0.13.3",
    "aws-sdk-mock": "~4.5.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-private-methods",
      "@babel/plugin-proposal-private-property-in-object"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "public/*"
    ],
    "delay": "2500"
  }
}
