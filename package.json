{
  "name": "miroweb",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev_build": "webpack --config ./webpack.config.js --mode development",
    "build": "webpack --config ./webpack.config.js --mode production",
    "build-backend": "npm run build && webpack --config ./webpack.config.server.js --mode production",
    "up": "npm run dev_build && node -r dotenv/config -r @babel/register -r regenerator-runtime/runtime src/boot.js",
    "start": "nodemon --exec 'npm run up'"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "~4.0.4",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1",
    "compression": "~1.7.4",
    "helmet": "~3.20.0",
    "@godaddy/terminus": "~4.1.2",
    "aws-sdk": "~2.509.0",
    "firebase-admin": "~8.3.0"
  },
  "devDependencies": {
    "nodemon": "~1.19.1",
    "@babel/core": "~7.5.5",
    "@babel/node": "~7.5.5",
    "@babel/cli": "~7.5.5",
    "@babel/preset-env": "~7.5.5",
    "@babel/register": "~7.5.5",
    "babel-loader": "~8.0.6",
    "webpack": "~4.39.1",
    "webpack-cli": "~3.3.6",
    "copy-webpack-plugin": "~5.0.4",
    "remove-files-webpack-plugin": "~1.1.3",
    "dotenv": "~8.0.0",
    "webpack-node-externals": "~1.7.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "public/*"
    ],
    "delay": "2500"
  }
}
